while True:
    memelist = {'КРИНЖ': 'что-то очень страное или стыдное', 'ЛОЛ': 'что-то очень смешное', 'РОФЛ': 'шутка', 'ЩИЩ': 'одобрение или восторг', }
    word = input("Введите непонятное слово (большими буквами!): ")
    if word in memelist.keys():
        print(memelist[word])
    else:
        print('простите такого слого не найдено')
